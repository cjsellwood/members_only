<%- layout("layout/boilerplate") %>
<h1>Members Only</h1>
<% if (currentUser) { %>
<p>User: <%= currentUser.username %></p>
<p>Password: <%= currentUser.password %></p>
<p>Is Member: <%= currentUser.isMember %></p>
<p>Is Admin: <%= currentUser.isAdmin %></p>
<% } else { %>
<p><a href="/login">Login</a> to see messages</p>
<% } %>
 <% if (currentUser && currentUser.isMember) {%>
<p>You are a member</p>
<% } else if (currentUser && !currentUser.isMember) { %>
<p><a href="/membership">Become a member</a> to see authors</p>
<% } %>
